{
    "name": "kill-ring",
    "displayName": "kill-ring",
    "description": "Emacs' kill-ring in VS Code",
    "version": "0.0.1",
    "publisher": "dvkt",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.kill",
        "onCommand:extension.yank"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.kill",
                "title": "Kill"
            },
            {
                "command": "extension.yank",
                "title": "Yank"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
